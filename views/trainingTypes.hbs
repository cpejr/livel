<!-- Modal -->
<div class="modal fade  id="stimulus" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
  <div class="modal-dialog modal-dialog-centered" role="document">
    <div class="modal-content modalstimulus">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <span class="boxRightUpperRadius1">
          {{treinos.Periodizacao}}
        </span>
      </div>
    </div>
  </div>
</div>

<body>
